{
  "name": "mini-pm",
  "version": "1.0.8",
  "description": "一款mini性能优化监控平台",
  "source": "src/index.ts",
  "main": "dist/mini-pm.js",
  "exports": "./dist/mini-pm.modern.js",
  "module": "dist/mini-pm.module.js",
  "types": "dist/typings/index.d.ts",
  "unpkg": "dist/mini-pm.umd.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle && npm run api:run && npm run api:doc",
    "dev": "microbundle watch",
    "example": "parcel serve ./examples/index.html -p 12345",
    "api:init": "api-extractor init",
    "api:run": "api-extractor run --local --verbose",
    "api:doc": "typedoc --out docs src",
    "sonar": "./sonar.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@microsoft/api-extractor": "^7.15.2",
    "@types/css-font-loading-module": "^0.0.4",
    "microbundle": "^0.13.0",
    "parcel-bundler": "^1.12.5",
    "scripty": "^2.0.0",
    "typedoc": "^0.20.36",
    "typescript": "4.2"
  },
  "dependencies": {
    "fflate": "^0.7.0",
    "performance": "^1.4.0",
    "src": "^1.1.2",
    "types": "^0.1.1"
  }
}
